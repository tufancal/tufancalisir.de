STORYBLOK_TOKEN=
STORYBLOK_SPACE_ID=

# Public token for Storyblok Bridge (same value as STORYBLOK_TOKEN)
# Required for visual editor to authenticate - this is a preview token, safe to expose
PUBLIC_STORYBLOK_TOKEN=

# Enable Storyblok Bridge for live editing (true for local dev, staging; false for production)
ENABLE_STORYBLOK_BRIDGE=true

# EmailJS Configuration
# Get these from https://dashboard.emailjs.com/
EMAILJS_SERVICE_ID=
EMAILJS_TEMPLATE_ID=
EMAILJS_PUBLIC_KEY=